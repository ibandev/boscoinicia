{% block form_row %}
{% spaceless %}
    <div class="form_row">
    	<div class="form_label">
    	    {{ form_label(form) }}
    	</div>
    	<div class="form_widget">
    	    {{ form_widget(form) }}
    	</div> 
        <div class="form_errors">
            {{ form_errors(form) }}
        </div>  
    </div>
{% endspaceless %}
{% endblock form_row %}